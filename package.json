{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "companion",
  "title": "Companion",
  "description": "A random assortment of utility commands.",
  "author": "robbiehobby",
  "icon": "extension-icon.png",
  "commands": [
    {
      "name": "datetime",
      "title": "Dates & Times",
      "description": "Transform dates using natural language.",
      "mode": "view",
      "icon": "extension-icon.png"
    },
    {
      "name": "orb",
      "title": "Menu Bar OrbStack",
      "description": "Show the current OrbStack data location in the menu bar.",
      "mode": "menu-bar",
      "interval": "10s"
    },
    {
      "name": "transform",
      "title": "Transform Text",
      "description": "Transform the selected text.",
      "mode": "view",
      "icon": "extension-icon.png"
    },
    {
      "name": "wifi",
      "title": "Menu Bar Wi-Fi",
      "description": "Show the current Wi-Fi SSID in the menu bar.",
      "mode": "menu-bar",
      "interval": "10s"
    }
  ],
  "scripts": {
    "dev": "ray develop",
    "build": "ray build && ray build -o ./public"
  },
  "devDependencies": {
    "@raycast/api": "^1.85.2",
    "@raycast/utils": "^1.17.0",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "chrono-node": "^2.7.7",
    "dayjs": "^1.11.13",
    "execa": "^9.5.2",
    "typescript": "^5.4.5"
  }
}
